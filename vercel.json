{
    "version": 2,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "backend/public/$1"
        }
    ],
    "functions": {
        "backend/server.js": {
            "maxDuration": 10
        }
    }
}